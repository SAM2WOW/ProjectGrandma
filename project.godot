; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ProjectGrandma"
run/main_scene="res://levels/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://scripts/Global.gd"
MusicPlayer="*res://scripts/MusicPlayer.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://arts/UI/player cursor/Hand young v3.png"
mouse_cursor/custom_image_hotspot=Vector2(8, 8)

[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Objects"
2d_physics/layer_3="Environment"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
